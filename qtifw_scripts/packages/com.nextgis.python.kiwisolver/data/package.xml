<?xml version="1.0" encoding="UTF-8"?>
<Package root="py_kiwisolver">
    <mac>
        <path src="Library/lib/python2.7/site-packages/*.py" dst="Library/Python/2.7/site-packages/"/>
        <path src="Library/lib/python2.7/site-packages/*.so" dst="Library/Python/2.7/site-packages/"/>
    </mac>
    <win>
        <path src="Lib/site-packages/*.py" dst="lib/python27/site-packages"/>
        <path src="Lib/site-packages/*.pyd" dst="lib/python27/site-packages"/>
    </win>
</Package>
